          "Q44": "{% $Q44_response %}",
          "Q45": "{% $Q45_response %}",
          "Q46": "{% $Q46_response %}",
          "Q47": "{% $Q47_response %}",
          "R6": "{% $R6_response %}",
          "Q48": "{% $Q48_response %}",
          "Q49": "{% $Q49_response %}",
          "Q51a": "{% $Q51a_response %}",
          "Q51b": "{% $Q51b_response %}",
          "RX": "{% $RX_response %}",
          "passed_data": "{% $passed_data %}",
          "algorithm": "Medication-Warfarin",
          "page-embeddings-file": "{% $embeddings_key %}"
        }
      },
      "Retry": [
        {
          "ErrorEquals": [
            "States.ALL"
          ],
          "IntervalSeconds": 1,
          "MaxAttempts": 1,
          "BackoffRate": 2,
          "JitterStrategy": "FULL"
        }
      ],
      "Catch": [
        {
          "ErrorEquals": [
            "States.ALL"
          ],
          "Next": "Fail State"
        }
      ],
      "TimeoutSeconds": 120,
      "End": true
    },
    "Fail State": {
      "Type": "Fail",
      "Error": "$['Error']",
      "Cause": "$['Cause']"
    }
  }
