          "Next": "Fail State"
        }
      ],
      "TimeoutSeconds": 120,
      "Next": "Q24a Choice"
    },
    "Q24a Choice": {
      "Type": "Choice",
      "Choices": [
        {
          "Next": "QOther24a",
          "Condition": "{% $contains($Q24a_response.Answer, 'other') %}"
        }
      ],
      "Default": "RL12 Choice"
    },
    "QOther24a": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
