      "Choices": [
        {
          "Next": "Q29 Pass",
          "Condition": "{% $Q29_response.Answer = 'yes' %}"
        }
      ],
      "Default": "Set New Data to Pass"
    },
    "Q29 Pass": {
      "Type": "Pass",
      "Assign": {
        "OOI_EVENT": "{% true %}"
      },
      "Next": "Set New Data to Pass"
    },
    "Set New Data to Pass": {
      "Type": "Pass",
      "Assign": {
        "passed_data": "{% $merge([$passed_data, {\"OOI_EVENT\": $OOI_EVENT}]) %}"
      },
      "Next": "Collect and Upload"
    },
    "Collect and Upload": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
      "Output": "{% $states.result.Payload %}",
      "Arguments": {
        "FunctionName": "arn:aws:lambda:us-east-1:864981749938:function:collectAndUpload:$LATEST",
        "Payload": {
          "OOI_EVENT": "{% $OOI_EVENT %}",
          "Q24": "{% $Q24_response %}",
          "Q24a": "{% $Q24a_response %}",
          "QOther24a": "{% $QOther24a_response %}",
          "Q24b": "{% $Q24b_response %}",
          "Q25": "{% $Q25_response %}",
          "Q26": "{% $Q26_response %}",
          "Q27": "{% $Q27_response %}",
          "Q28": "{% $Q28_response %}",
