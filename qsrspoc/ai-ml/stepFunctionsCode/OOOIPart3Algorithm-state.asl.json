      },
      "Next": "RL12 Choice",
      "Retry": [
        {
          "ErrorEquals": [
            "Lambda.ServiceException",
            "Lambda.AWSLambdaException",
            "Lambda.SdkClientException",
            "Lambda.TooManyRequestsException"
          ],
          "IntervalSeconds": 1,
          "MaxAttempts": 1,
          "BackoffRate": 2,
          "JitterStrategy": "FULL"
        }
      ],
      "Catch": [
        {
          "ErrorEquals": [
            "States.ALL"
          ],
          "Next": "Fail State"
        }
      ],
      "TimeoutSeconds": 120
    },
    "RL12 Choice": {
      "Type": "Choice",
      "Choices": [
        {
          "Next": "Q24b",
          "Condition": "{% $discharge_status = 'died' %}"
        }
      ],
      "Default": "Q25"
    },
